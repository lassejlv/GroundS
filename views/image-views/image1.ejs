      crossorigin="anonymous"
    ></script>
    <link rel="stylesheet" href="css/img-view/style.css" /> 
    <link rel="stylesheet" href="css/templat-css-styles.css" />
    <link rel="icon" href="css/3991.png" />
    <!--Nav Bar-->
    <section class="index-nav-bar">
      <!--  <nav class="navbar navbar-expand-lg navbar-light bg-light">-->
      <nav class="navbar navbar-dark bg-dark">
        <div class="container-fluid">
          <a class="navbar-brand" href="#"
            ><i class="fas fa-images"></i> Ground's</a
          >
          <button
            class="navbar-toggler"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <li class="nav-item">
                <a class="nav-link active" aria-current="page" href="#"
                  ><i class="fas fa-home"></i> Home</a
                >
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#"
                  ><i class="far fa-question-circle"></i> About</a
                >
              </li>
            </ul>
            <form class="d-flex">
              <input
                class="form-control me-2"
                type="search"
                placeholder="Search"
                aria-label="Search"
              />
              <button class="btn btn-outline-light" type="submit">
                <i class="fas fa-search"></i>
              </button>
            </form>
          </div>
        </div>
      </nav>
    </section>
    <!--Scripts-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4" crossorigin="anonymous"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4"
      crossorigin="anonymous"
    ></script>
    <script src="js/script.js"></script>
    <script
      src="https://kit.fontawesome.com/4b0c2715e4.js"
      crossorigin="anonymous"
    ></script>
     <!--Card section-->
     <section class="index-cards">
        <div class="container py-3">
          <!-- Card Start -->
          <div class="card" style="width: 18rem;">
            <img src="/img/Backgrounds/fadd.jpg" class="card-img-top" alt="Background Image">
            <div class="card-body">
              <a href="img/Backgrounds/fadd.jpg" title="Download Image" download="download" class="btn btn-outline-dark"><i class="fas fa-download"></i> Download Image</a>
              <button  title="Like Image" class="btn btn-outline-danger"><i class="fas fa-heart"></i></button>
            </div>
          </div>
      </section>
